# Google Sign-In Configuration
# Copy this file to .env and update the values

# Required: Web Client ID from Google Cloud Console
GOOGLE_WEB_CLIENT_ID=790161106297-lbups6i0ih4p3mb2i0qprja81pmdm21s.apps.googleusercontent.com

# Optional: iOS Client ID (if different from GoogleService-Info.plist)
# GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com

# Optional: Hosted Domain (for G Suite accounts)
# GOOGLE_HOSTED_DOMAIN=

# Optional: Offline Access (for server-side API access)
GOOGLE_OFFLINE_ACCESS=false

# Optional: Force Code for Refresh Token (Android)
GOOGLE_FORCE_CODE_FOR_REFRESH_TOKEN=false

# Optional: Account Name (Android)
# GOOGLE_ACCOUNT_NAME=

# Optional: Profile Image Size (iOS)
GOOGLE_PROFILE_IMAGE_SIZE=120

# Optional: OpenID Realm (iOS)
# GOOGLE_OPENID_REALM=

# Optional: Google Service Plist Path (iOS)
# GOOGLE_SERVICE_PLIST_PATH=
